			System.out.println(player.getPlayer());
			
			HoleArray boopArray = new HoleArray();
			boopArray.startGame();
			System.out.println("New Game!!!!!!!!!!!!!! Capture");
			boopArray.pickHole(5);
			boopArray.pickHole(12);
			boopArray.pickHole(0);
			System.out.println(boopArray.getArrayObject(5).getNumBeads() + " Should be 0");
			System.out.println(boopArray.getArrayObject(7).getNumBeads() + " Should be 0");
			System.out.println(boopArray.getArrayObject(6).getNumBeads() + " Should be 7");
